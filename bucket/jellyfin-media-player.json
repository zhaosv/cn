    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/jellyfin/jellyfin-media-player/releases/download/v$version/JellyfinMediaPlayer-$version-windows-x64.exe"
            },
            "32bit": {
